.Footer {
    width:100%;
    height:50px;
    position:fixed;
    bottom:0;
    left:0; 
    z-index:106;
    background:#ffffff;
}
.Footer__nav{
     width:100%;
     height:50px;
    background:#ffffff;
     display:flex;
      align-items: center;
    justify-content: space-between;
    overflow:hidden;
}
.Footer__goshopcart{
    width:20%;
    text-align:center;
     display:flex;
       align-items: center;
    justify-content: center;
    position:relative;
   
}

.Footer__icon{
     width:22px;
     height:19px;
     margin:5px auto 0 auto;
     align-items: center;
 
}
.Footer__text{
     color:#333;
     font-size:12px;
    line-height:1.8;
      align-items: center;
      justify-content: center;
}
.Footer__addshopcart{
    flex:1;
    display:flex;
    height:100%;
    font-size:16px;
    background:#ff6900;
    align-items: center;
    justify-content: center;
    color:#ffffff;
}